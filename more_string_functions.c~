#include "shell.h"

/**
 * _strcomp - compares two strings if they are the same
 * @s1: string 1
 * @s2: string 2
 *
 * Return: 0 on success
 */
int _strcomp(char *s1, char *s2)
{
	int len_s1 = _strlen(s1), len_s2 = _strlen(s2), i, j;

	if (len_s1 == len_s2)
	{
		for (i = 0; s1[i]; i++)
		{
			if (s1[i] == s2[i])
				j++;
		}
		if (i == j)
			return (0);
	}
	return (1);
}
